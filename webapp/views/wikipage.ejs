<!-- views/wikipage.ejs -->

<!DOCTYPE html>

<html lang="en">
<head>
    <!--Load the AJAX API-->
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.18/angular.js"></script>
    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript" src="js/wikipage.js"></script>
    <% include partials/head %>
</head>
<body class="container">
  <div class="main">
    <header>
      <% include partials/header %>
    </header>
    <h1 id="pagename" class="pagename"></h1>
    <div class="descriptioncard">
      <div id="description" class="text"></div>
    </div>
    <br>
    <div class="row">
      <div class="chart column">
        <div id="chart_div" class="chart"></div>
        <br>
        <div class="contentCard">
          <div class="text">Related WikiPages:</div>
        </div>
      </div>
      <div class="contentCard content column">
        <div id="articles" class="text"></div>
      </div>
    </div>
    <br>
    <footer>
      <% include partials/footer %>
    </footer>
  </div>
</body>
</html>
